<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.meeks.domain">

	<class name="Reserve" table="meeks_reserve" lazy="false">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="name" />
		<property name="phone" />
		<property name="startTime" />
		<property name="endTime" />

		<many-to-one name="house" column="houseId"/>
	</class>

</hibernate-mapping>
